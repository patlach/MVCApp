@{
    ViewData["Title"] = "Logs";
}

<style>
     table {
       border-spacing: 0 15px;
       align-self: center;
     }
     th {
       background-color: #4287f5;
       color: white;
     }
     th,
     td {
       width: 150px;
       text-align: center;
       border: 1px solid black;
       padding: 5px;
     }
     h2 {
       color: #4287f5;
     }
   </style>
 
<div class="text-center">
   <div>
       <b>История</b>
   </div>
   <br>
       <table>
           <tr>
               <td>Идентификатор</td>
               <td>Имя</td>
               <td>Дата регистрации</td>
           </tr>
           @foreach (var requst in Model)
               {
               <tr>
                   <td>@requst.Id</td>
                   <td>@requst.Date</td>
                   <td>@requst.Url</td>
               </tr>
           }
       </table>
</div>